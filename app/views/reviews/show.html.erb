<h1>User: <%= @review.user_name %></h1>
<h2><%= @review.rubies %> Rubies</h2>
<br>
<h3><%= @review.text %></h3>

<p><%= link_to "Edit Review", edit_product_review_path(@product, @review) %></p>
<p><%= link_to "Delete", product_review_path(@product, @review),
                         :data => {:confirm => "You sure?",
                                   :method => "delete"} %></p>

<p><%= link_to "Return to product", product_path(@product) %></p>
